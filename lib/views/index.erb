<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <% if artists.any? %>
      <section>
        <%= video %>
        <h3>Artists:</h3>
        <ul>
          <% artists.each do |artist| %>
            <li><%= artist  %></li>
          <% end %>
        </ul>
      </section>
    <% else %>
			<a href="http://turing.io">Turing School</a>
    <% end %>
    <h3>Stuff:</h3>
    <ul id='stuff'>
      <% list.each do |item| %>
        <li><%= item %></li>
      <% end %>
    </ul>
  </body>
</html>
